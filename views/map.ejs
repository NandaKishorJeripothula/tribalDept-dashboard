<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <link rel="apple-touch-icon" sizes="76x76" href="media/apple-icon.png">
    <link rel="icon" type="image/png" href="media/favicon.png">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>
        Map
    </title>
    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0, shrink-to-fit=no' name='viewport'
    />
    <!--     Fonts and icons     -->
    <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Roboto+Slab:400,700|Material+Icons"
    />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css">
    <!-- CSS Files -->
    <link rel="stylesheet" href="css/materialize.min.css">
    <link href="css/material-dashboard.css?v=2.1.0" rel="stylesheet" />
</head>

<body>
        <div id='header'></div>
        <div class="main-panel" id='#mainPanel'>
                <!-- Navbar -->
              <nav class="navbar navbar-expand-lg navbar-transparent navbar-absolute fixed-top ">
                  <div class="container-fluid">
                  <div class="navbar-wrapper">
                    <a class="navbar-brand">Map</a>
                  </div>
                  <button class="navbar-toggler" type="button" data-toggle="collapse" aria-controls="navigation-index" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="navbar-toggler-icon icon-bar"></span>
                    <span class="navbar-toggler-icon icon-bar"></span>
                    <span class="navbar-toggler-icon icon-bar"></span>
                  </button>
                  <div class="collapse navbar-collapse justify-content-end">
                    <ul class="navbar-nav">
                      <li class="nav-item dropdown">
                        <a class="nav-link" href="http://example.com" id="navbarDropdownMenuLink" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                          <i class="material-icons">person</i>
                          <span class="notification">4</span>
                          <p class="d-lg-none d-md-block">
                            User Profile
                          </p>
                        </a>
                        <div class="dropdown-menu dropdown-menu-right" aria-labelledby="navbarDropdownMenuLink">
                          <a class="dropdown-item" href="#">Id:<%= user.id %></a>
                          <a class="dropdown-item" href="#">UserName:<%= user.username %></a>
                          <a class="dropdown-item" href="#">Password:<%= user.password %></a></a>
                          <a class="dropdown-item" href="/logout">Logout</a>
                        </div>
                      </li>
                    </ul>
                  </div>
                </div>
              </nav>
              <!-- End Navbar -->
              <div id="institutionsMap"></div>
                <div class="content">
                    <!-- ====START Landing AREA==== -->
                    <section id="landing" class="map-area">
                            <div class="contact_google_map">
                                <div id="institutionsMap">
                                    <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3785.0927955645234!2d79.1269655140532!3d18.434095387459404!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x3bccd93cda1331c9%3A0xcfce83034fa221cb!2sBus+Stand+Cir%2C+Mukarampura%2C+Karimnagar%2C+Telangana+505001!5e0!3m2!1sen!2sin!4v1520849731383" width="600" height="450" frameborder="0" style="border:0" allowfullscreen></iframe>
                                </div>
                            </div>
                            <div class="contact-area">
                                <div class="container">
                                    <div class="contact_text col-md-6">
                                        <div class="section_title">
                                            <h2 class="text-left">Towers Near Me???</h2>
                                            <p>We Help You To Find Out</p>
                                        </div>
                                        <form action=" " method="post">
                                            <div class="row jsSubmit_button">
                                                <div class="input-field col-md-6">
                                                    <div class="form-group" id='inputSection'>
                                                        <select name="color" id="area" onChange="mapLoader(value);" class="form-control" class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                                                        <option value="null">City</option>
                                                        <option value="Karimnagar Kaman" selected>Karimnagar Kaman</option>
                                                        <option value="Karimnagar Circle">Karimnagar Circle</option>
                                                        <option value="Karimnagar Ramnagar">Karimnagar RamNagar</option>
                                                        <option value="Hyderabad JBS">Hyderabad JBS</option>
                                                        <option value="Hyderabad JUBLEE HILLS">Hyderabad JUBLEE</option>
                                                        <option value="Hyderabad Masab Tank">Hyderabad Masab Tank</option>
                                                        <option value="Warangal">Warangal</option>
                                                        <option value="Huzurabad">Huzurabad</option>
                                                        </select>
                                                    </div>
                                                </div>
                                                <div class="col-md-12">
                                                    <button type="button" class="th_bt btn waves-effect" id="find_submitBtn"name="find-btn">
                                                        Find<i class="zmdi zmdi-help"></i>
                                                    </button>
                                                </div>
                                            </div>
                                        </form>
                                    </div>
                                </div> 
                            </div>
                        </section>
                        <!-- ====END Landing AREA==== -->
                <footer class="footer" id='footer_JS'>
                </footer>
        </div>
        <div class="wrapper" id='sideMenu'>
        </div>
        </div>
    <!--CORE JS FILES HERE-->
    <script src="js/ui.js"></script>
    <!--   Core JS Files   -->
    <script src="js/core/jquery.min.js" type="text/javascript"></script>
    <script src="js/core/popper.min.js" type="text/javascript"></script>
    <script src="js/core/bootstrap-material-design.min.js" type="text/javascript"></script>
    <script src="js/plugins/perfect-scrollbar.jquery.min.js"></script>
    <!--  Google Maps Plugin    -->
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAkOKR5LPxH76BxBWR6poRqDxRrIVc20Dc"></script>
    <!-- Chartist JS -->
    <script src="js/plugins/chartist.min.js"></script>
    <!--  Notifications Plugin    -->
    <script src="js/plugins/bootstrap-notify.js"></script>
    <script src="js/demo.js"></script>
    
    <!-- Control Center for Material Dashboard: parallax effects, scripts for the example pages etc -->
    <script src="js/material-dashboard.js" type="text/javascript"></script>
    <script src="js/materialize.min.js" type="text/javascript"></script>
    <!-- Material Dashboard -->
    <!--ACCESS_POINTS-->
    <script src="js/accesspoints.js"></script>
    <script src="js/map.js"></script>
    
</body>

</html>